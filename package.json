{
  "name": "linter-apache-configtest",
  "main": "./lib/linter-apache-configtest",
  "version": "0.0.0",
  "description": "A short description of your package",
  "keywords": [
  ],
  "repository": "https://github.com/atom/linter-apache-configtest",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-package-deps": "^4.0.1",
    "atom-linter": "^5.0.1"
  },
  "devDependencies": {
    "eslint": "^2.11.0",
    "babel-eslint": "^6.0.0"
  },
  "packageDeps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "atom": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        "error",
        { "argsIgnorePattern": "^_" }
      ]
    }
  }
}
